<?xml version="1.0"?>

<launch>
  <!-- Start Robotiq force-torque sensor -->
  <!-- Note that this must be run onboard the robot. -->
  <node name="ft_sensor" pkg="robotiq_force_torque_sensor" type="rq_sensor" respawn="true" output="screen" />

  <!-- Launch force info node. -->
  <node name="mm_force_control_node" pkg="mm_force_control" type="force.py" output="screen" />
</launch>
