<?xml version="1.0"?>

<!-- Single launch file for all onboard nodes. -->

<launch>
  <!-- Motion controller. -->
  <include file="$(find mm_motion_control)/launch/real/mm_motion_control.launch" />

  <!-- Robotiq force-torque sensor -->
  <node name="ft_sensor" pkg="robotiq_force_torque_sensor" type="rq_sensor" respawn="true" output="screen" />

  <!-- Gripper control -->
  <include file="$(find mm_gripper)/launch/gripper.launch" />
</launch>
